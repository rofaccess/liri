# This file is a bash script to compile code and reinstall locally.
# How to use
# > sh compile
# 'rake install' command is better alternative to 'sh compile' command

# Build gem
gem build liri.gemspec

# Install gem locally
# TODO The gem version is set manually here, the version must be obtained from lib/liri/version.rb
gem install ./liri-0.1.0.gem